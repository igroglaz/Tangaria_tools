        <SDLDir>$([System.IO.Path]::GetFullPath('$(SourceRoot)_SDL2\'))</SDLDir>
        <SDLLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)SDL2.lib'))</SDLLibrary>
